{
  "title": "parameter",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.inventory.model.workflow.Parameter",
  "type": "object",
  "required": [
    "type"
  ],
  "description": "parameters only for parameter type 'List'",
  "properties": {
    "type": {
      "$ref": "parameterType-schema.json"
    },
    "default": {
      "description": "this value has to have the data type of the 'type' attribute",
      "anyOf": [
        { "type": "string" },
        { "type": "number" },
        { "type": "boolean" },
        { "type": "array" }
      ],
      "javaType": "java.lang.Object"
    },
    "final": {
      "type": "string"
    },
    "listParameters": {
      "$ref": "listParameters-schema.json"
    }
  },
  "additionalProperties": false
}