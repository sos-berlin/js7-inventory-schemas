{
  "title": "job return code warning",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.inventory.model.job.JobReturnCodeWarning",
  "type": "object",
  "properties": {
    "warning": {
      	"anyOf": [
    		{
      			"type": "array",
      			"minItems": 1,
      			"items": {
        			"type": "integer"
      			}
      		},
      		{
      			"type": "string",
      			"pattern": "^-?[0-9]+(\\.\\.-?[0-9]+)?(, *-?[0-9]+(\\.\\.-?[0-9]+)?)*$"
      		}
      	]
    }
  },
  "additionalProperties": false
}