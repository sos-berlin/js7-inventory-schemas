{
  "title": "executable script",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.inventory.model.job.ExecutableScript",
  "type": "object",
  "extends" : {
    "$ref": "executable-schema.json"
  },
  "required": [
  	"script"
  ],
  "propertyOrder": [
    "TYPE",
    "script",
    "env",
    "v1Compatible"
  ],
  "description": "executable with fixed property 'TYPE':'ScriptExecutable'",
  "properties": {
    "script": {
	  "type": "string",
	  "minLength": 1
	},
    "env": {
      "$ref": "env-schema.json"
    },
	"v1Compatible": {
      "type": "boolean",
      "default": false
    },
    "login": {
		"type": "object",
		"javaType": "com.sos.inventory.model.job.ExecutableScriptLogin",
		"properties": {
			"credentialKey": {
				"type": "string"
			},
			"withUserProfile": {
				"type": "boolean",
				"default": false
			}
		},
		"additionalProperties": false
	}
  },
  "additionalProperties": false
}

