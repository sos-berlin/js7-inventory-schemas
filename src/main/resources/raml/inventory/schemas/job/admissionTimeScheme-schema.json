{
  "title": "admission time scheme",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "javaType": "com.sos.inventory.model.job.AdmissionTimeScheme",
  "type": "object",
  "required": [
    "periods"
  ],
  "properties": {
    "periods": {
		"type": "array",
		"items": {
			"$ref": "admissionTimePeriod-schema.json"
		}
    },
    "restrictedSchemes":  {
        "type": "array",
        "items": {
            "type": "object",
            "javaType": "com.sos.inventory.model.job.AdmissionRestrictedSchemes",
            "required": [
                "restriction",
                "period"
            ],
            "properties": {
                "restriction": {
                    "$ref": "admissionRestrictedScheme-schema.json"
                },
                "periods": {
                    "type": "array",
                    "items": {
                        "$ref": "admissionTimePeriod-schema.json"
                    }
                }
            },
            "additionalProperties": false
        }
    }
  },
  "additionalProperties": false
}